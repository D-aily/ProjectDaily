<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="daily.mapper.CartMapper">
 
 <!-- ~~~ -기본 CRUD SQL문 ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
 <insert id="insert">
 	insert into (cartnum,id,productnum,quantity) 
 		   values (LAST_INSERT_ID(),#{id},#{productnum},#{quantity}) 
 </insert>
 <select id="cselectList" resultType="vo.CartVO">
 	select
 	cartnum,id,productnum,quanttity,date from cart
 </select>
 	
 
 
 
</mapper>